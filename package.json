{
  "name": "akindcity6",
  "version": "1.0.0",
  "description": "Accessibility Map for Norwich",
  "scripts": {
    "start": "parcel data/**/* index.html --target browser",
    "start-debug": "parcel data/**/* index.html --target browser --log-level 4",
    "test": "echo \"Error: no test specified\" && exit 1",
    "predeploy": "parcel build index.html data/**/* --target browser --public-url /AKindCity6/  && rm dist/data/nolicence/*.*",
    "deploy": "gh-pages -d dist",
    "download-osm": "--node --experimental-modules ./src/downloader.mjs",
    "down": "node ./back-end/main.js"
  },
  "browser": "index.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robinkeith/AKindCity6.git"
  },
  "homepage": "https://robinkeith.github.io/AKindCity6",
  "author": "Robin Keith",
  "license": "MIT",
  "parcelCleanPaths": [
    "dist",
    ".cache"
  ],
