{
  "name": "akindcity6",
  "version": "1.0.0",
  "description": "Accessibility Map for Norwich",
  "scripts": {
    "start": "parcel data/**/* index.html --target browser",
    "start-debug": "parcel data/**/* index.html --target browser --log-level 4",
    "test": "echo \"Error: no test specified\" && exit 1",
    "predeploy": "parcel build index.html data/**/* --target browser --public-url /AKindCity6/",
    "deploy": "gh-pages -d dist",
    "download-osm": "node --experimental-modules ./src/downloader.mjs"
  },
  "browser": "index.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robinkeith/AKindCity6.git"
  },
  "homepage": "https://robinkeith.github.io/AKindCity6",
  "author": "Robin Keith",
  "license": "MIT",
  "parcelCleanPaths": [
    "dist",
    ".cache"
  ],
  "devDependencies": {
    "cssnano": "^4.1.10",
    "gh-pages": "^2.0.1",
    "marked": "^0.6.2",
    "parcel-plugin-clean-easy": "^1.0.2",
    "sass": "^1.22.1"
  },
  "dependencies": {
    "@bagage/leaflet.restoreview": "^1.0.1",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@turf/turf": "^5.1.6",
    "better-title-case": "^1.0.1",
    "bootstrap": "^4.3.1",
    "country-language": "^0.1.7",
    "i18next-client": "^1.11.4",
    "jquery": "^3.4.1",
    "leaflet": "^1.5.1",
    "leaflet-ajax": "^2.1.0",
    "leaflet-boundsawarelayergroup": "^1.2.2",
    "leaflet-control-geocoder": "^1.8.2",
    "leaflet-easybutton": "^2.4.0",
    "leaflet-easyprint-forked": "^2.1.8",
    "leaflet-edgebuffer": "^1.0.6",
    "leaflet-extra-markers": "^1.1.0",
    "leaflet-fullscreen": "^1.0.2",
    "leaflet-providers": "^1.8.0",
    "leaflet-routing-machine": "^3.2.12",
    "leaflet-search": "^2.9.8",
    "leaflet-sidebar-v2": "^3.1.1",
    "leaflet-toolbar": "^0.4.0-alpha.2",
    "leaflet.locatecontrol": "^0.67.0",
    "moment": "^2.24.0",
    "opening_hours": "^3.5.0",
    "popper.js": "^1.15.0",
    "store2": "^2.7.1"
  }
}
